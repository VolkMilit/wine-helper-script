#compdef wine

_wine() {
  local commands
  commands=(
	'-h:See wine script help'
    '--help:Display help and exit'
    '--version:Output version information and exit'
    '--patches:Output patch information and exit'
    '--check-libs:Checks if shared libs are installed'
    '--gtk:Use gtk to choose wine prefix'
    'winecfg:Wine configure'
    'regedit:Wine RegEdit (c)'
    'uninstaller:Wine uninstalletion program'
    $(ls | grep exe)
  )

  if (( CURRENT == 2 )); then
    _describe -t commands 'commands' commands
  fi

  return 0
}

_wine
